generator client {
  provider = "prisma-client-js"
  output   = "../src/generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Demo {
  id            Int    @id @default(autoincrement())
  gdNo          String
  policeStation String
  vadiName      String
  accusedName   String
  underSection  String
  status        String
  place         String
}
